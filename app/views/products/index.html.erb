<div class="pad-top pad-bottom">
  <h1>Products</h1>
</div>

<table>
  <%#thead>
    <tr>
      <th>Name</th>
      <th>Description</th>
      <th>Image url</th>
      <th>Colour</th>
      <th>Price</th>
      <th colspan="3"></th>
    </tr>
  </thead%>
<% cache(cache_key_for_products) do %>
    <%= render partial: 'product' %>
<% end %>
</table>
